{
  "extends": "next/core-web-vitals",
  "plugins": ["eslint-plugin-import-helpers"],
  "rules": {
    "no-unused-vars": "error",
    "import-helpers/order-imports": [
      "warn",
      {
        "newlinesBetween": "always",
        "groups": ["module", ["/^@/", "parent"], ["sibling", "index"]],
        "alphabetize": { "ignoreCase": true }
      }
    ]
  }
}
